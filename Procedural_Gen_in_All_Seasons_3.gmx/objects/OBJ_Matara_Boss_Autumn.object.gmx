            <kind>1</kind>
            <string>//sort global.leafArray by it's 3rd index
//quicksort(0, 999);

/*//create a new empty array
for (l = 999; l &gt;= 0; l--){
    for(k = 6; k &gt;= 0; k--){
        tempArray[k, l] = 0;
    }
}

//take the top 500 (500 closest bullets) and save that to the array
for (i = 0; i &lt; 500; i++){
    for(k = 0; k &lt; 7; k++){
        tempArray[k, l] = global.leafArray[k, l];
    }
}
*/

//Now give birth to new bullets, based on the averages of the previous
for (i = 0; i &lt; 500; i++){
    for(k = 0; k &lt; 7; k++){
        thisK1 = floor(random(500));
        if (k == 3){ 
            global.leafArray[k, i + 500] = global.leafArray[k, thisK1];
        } else {
            thisK2 = floor(random(500));
            global.leafArray[k, i + 500] = (global.leafArray[k, thisK1] + global.leafArray[k, thisK2])/2;
        }
    }
}

//Finally, spawn the next wave in!
for (l = 0; l &lt; 1000; l++){

    //But sometimes you still have to randomize it;
    randomThis = floor(random(10));
    
    if(randomThis == 0){
        red = ceil(random(55) + 200);
        green = ceil(random(100) + 100);
        blue = ceil(random(25));
        curvature = random(20)-10;
        leaf_speed = random(5) + 4;
        leaf_direction = random(720) - 360;
    } else {
        red = global.leafArray[0, l];
        green = global.leafArray[1, l];
        blue = global.leafArray[2, l];
        curvature = global.leafArray[4, l];
        leaf_speed = global.leafArray[5, l];
        leaf_direction = global.leafArray[6, l];
    }
    with(instance_create(random(room_width), room_height * (1/5), OBJ_Falling_Leaf)){
        i = other.l
        red = other.red;
        green = other.green;
        blue = other.blue;
        curvature = other.curvature;
        speed = other.leaf_speed;
        direction_mod = other.leaf_direction;
    }
}

