        attempted_direction += 180;
    }

    x_attmept = lengthdir_x(burst_radius,attempted_direction);
    y_attempt = lengthdir_y(burst_radius,attempted_direction);
    withinRange = ((x + x_attempt &gt; 64 &amp;&amp; x + x_attempt &lt; room_width - 64) &amp;&amp; (y + y_attempt &gt; 48 &amp;&amp; y + y_attempt &lt; room_width / 2));
}

if(withinRange){
    move_towards_point(x + x_attempt, y + y_attempt, (burst_radius/30));
    friction = 0.15;
    alarm_set(5, 135);
} else {
    alarm_set(5, 30);
}
